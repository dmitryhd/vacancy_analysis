#!/usr/bin/env python3

""" Entry point migration application. """

import sys
sys.path.append('..')
import vacan.processor.migrate as migrate
import vacan.processor.data_model as dm

if __name__ == '__main__':
    migrator = migrate.Migrator()
    db_manager = dm.DBEngine(sys.argv[2])
    migrator.migrate(sys.argv[1], db_manager)
